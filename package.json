{
  "name": "angular-cli-seed",
  "version": "4.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-cli-seed:serve-ssr",
    "serve:ssr": "node dist/angular-cli-seed/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-cli-seed:server:production",
    "prerender": "ng run angular-cli-seed:prerender",
    "clean": "rimraf ./dist/",
    "release": "standard-version -a && git push --follow-tags origin master",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean && npm run build -- --statsJson=true && webpack-bundle-analyzer ./dist/angular-cli-seed/browser/stats-es2015.json",
    "compodoc": "compodoc -p tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0-next.2",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~12.0.0-next.2",
    "@angular/compiler": "~12.0.0-next.2",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~12.0.0-next.2",
    "@angular/localize": "^12.0.0-next.2",
    "@angular/platform-browser": "~12.0.0-next.2",
    "@angular/platform-browser-dynamic": "~12.0.0-next.2",
    "@angular/platform-server": "~12.0.0-next.2",
    "@angular/router": "~12.0.0-next.2",
    "@angular/service-worker": "~12.0.0-next.2",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@nguniversal/express-engine": "^11.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.6.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "primeicons": "^4.1.0",
    "primeng": "^11.2.3",
    "rxjs": "~6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~12.0.0-next.2",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.2.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.31",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.0",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
